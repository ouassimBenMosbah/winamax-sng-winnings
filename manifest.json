{
  "manifest_version": 2,

  "name": "Winamax sng winnings",
  "description": "This extension computes a winamax player earnings on sngs.",
  "version": "1.0",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "32": "icon32.png",
